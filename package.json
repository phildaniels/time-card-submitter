{
  "name": "time-card-submitter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug:server": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --check --ignore-path .gitignore .",
    "format:fix": "prettier --write --ignore-path .gitignore .",
    "nest:prebuild": "rimraf dist",
    "nest:build": "nest build",
    "nest:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "nest:start": "nest start --path pages/api/server",
    "nest:start:dev": "nest start --watch",
    "nest:start:debug": "nest start --debug --watch",
    "nest:start:prod": "node dist/main",
    "nest:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "nest:test": "jest",
    "nest:test:watch": "jest --watch",
    "nest:test:cov": "jest --coverage",
    "nest:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "nest:test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@azure/msal-browser": "^2.27.0",
    "@azure/msal-react": "^1.4.3",
    "@emotion/react": "^11.10.0",
    "@emotion/server": "^11.10.0",
    "@mantine/core": "^5.1.3",
    "@mantine/hooks": "^5.1.3",
    "@mantine/next": "^5.1.3",
    "@nestjs/common": "^9.0.9",
    "@nestjs/core": "^9.0.9",
    "@tabler/icons": "^1.78.1",
    "@types/axios": "^0.14.0",
    "axios": "^0.27.2",
    "install": "^0.13.0",
    "nest-next": "^10.0.0",
    "next": "12.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.6"
  },
  "devDependencies": {
    "@types/node": "18.0.4",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.7",
    "eslint": "8.19.0",
    "eslint-config-next": "12.2.2",
    "eslint-config-prettier": "^8.5.0",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.6",
    "typescript": "4.7.4"
  }
}
